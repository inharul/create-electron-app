{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "outDir": "dist"
  },
  "exclude": ["dist", "template", "templates"]
}
